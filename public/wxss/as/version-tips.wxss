.version-tips {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 8888;
  background: #eee;
}
.version-tips swiper {
  width: 100%;
  height: 100%;
}
.version-tips .icon_close {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  background: rgba(255, 255, 255, 0.2);
  color: #e87;
  text-align: center;
  height: 50rpx;
  font-size: 30rpx;
  padding: 4rpx 10rpx;
  border-radius: 5rpx;
  line-height: 50rpx;
  z-index: 8889;
}
.version-tips .p1 .vt-content {
  width: 400rpx;
  margin: 20vw auto;
  text-align: center;
}
.version-tips .p1 .vt-title {
  width: 100%;
  text-align: center;
  padding-top: 50rpx;
  margin: 0 auto;
  color: #e87;
  position: absolute;
  bottom: 20vw;
  left: 0;
}
.version-tips .p1 .heartbeat-loader {
  z-index: 34;
  margin: 30vh auto;
}
.version-tips .p1 .vt-heart-bg {
  position: absolute;
  width: 70vw;
  height: auto;
  right: 5vw;
  top: 20vw;
  z-index: 33;
}
.version-tips .p1 .vt-heart-bg1 {
  position: absolute;
  width: 70vw;
  height: auto;
  left: 5vw;
  top: 5vw;
  z-index: 32;
}
.version-tips .p1 .vt-button {
  background: #e87;
  width: 400rpx;
  height: 80rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 40rpx;
  text-align: center;
  line-height: 80rpx;
  padding-top: 0;
  left: 50%;
  margin-left: -200rpx;
}
